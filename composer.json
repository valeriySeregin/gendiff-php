{
    "name": "valeraseregin/gendiff-php",
    "description": "php study project",
    "type": "library",
    "license": "MIT",
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "psr-4": {
          "Php\\Package\\": "src",
          "Php\\Package\\Tests\\": "tests"
        },
        "files": [
            "src/DiffGenerator.php",
            "src/Parsers/JsonParser.php",
            "src/Parsers/YamlParser.php",
            "src/Parsers.php",
            "src/Formatters/Pretty.php",
            "src/Formatters/Plain.php",
            "src/Formatters/Json.php",
            "src/Formatters.php"
        ]
    },
    "authors": [
        {
            "name": "Valeriy Seregin",
            "email": "valera-s-38@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^5.1",
        "funct/funct": "^1.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "^9.4",
        "phpstan/phpstan": "^0.12.58"
    }
}
